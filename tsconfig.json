{
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
        "./tests",
        "./node_modules",
        "./dist"
      ],
     
    "compilerOptions": {
      "target": "es2022",
      "module": "commonjs",
      "rootDir": "./src",
      "declaration": true,
      "noImplicitAny":true,
      "strictNullChecks":true,
      "noImplicitReturns":true,
      "outDir": "./dist"
    }
  }
 
 
 